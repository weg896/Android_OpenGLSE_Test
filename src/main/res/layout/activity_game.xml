<RelativeLayout xmlns:andorid="http://schemas.android.com/apk/res/android"
    andorid:layout_width="match_parent"
    andorid:layout_height="match_parent">


    <TextView
        andorid:layout_width="wrap_content"
        andorid:layout_height="wrap_content"
        andorid:textAppearance="?android:attr/textAppearanceMedium"
        andorid:text="Medium Text"
        andorid:id="@+id/textView"
        andorid:layout_marginTop="201dp"
        andorid:layout_alignParentTop="true"
        andorid:layout_alignParentStart="true" />

    <CheckBox
        andorid:layout_width="wrap_content"
        andorid:layout_height="wrap_content"
        andorid:text="New CheckBox"
        andorid:id="@+id/checkBox"
        andorid:layout_centerVertical="true"
        andorid:layout_alignParentStart="true" />
</RelativeLayout>
